{
  "name": "note-machina",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "lint": "eslint --ext .ts --ext .js --ext .tsx ./src",
    "migrate": "node -r dotenv/config ./lib/cli/migrate.js",
    "migrate:dev": "ts-node -r dotenv/config ./src/cli/migrate.ts",
    "test": "jest",
    "build": "npm run build:source && npm run build:webview && npm run build:next",
    "build:source": "tsc",
    "build:webview": "tsc -p ./src/webview/tsconfig.build.json",
    "build:next": "next build ./src/webview",
    "start": "node -r dotenv/config ./lib/serve.js",
    "dev": "nodemon -x ts-node -w src -i src/webview -r dotenv/config ./src/serve.ts"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --quiet"
    }
  },
  "dependencies": {
    "@machinat/auth": "file:machinat-auth-0.3.0-beta.2.tgz",
    "@machinat/core": "file:machinat-core-0.3.0-beta.3.tgz",
    "@machinat/dialogflow": "file:machinat-dialogflow-0.3.0-beta.2.tgz",
    "@machinat/http": "file:machinat-http-0.3.0-beta.2.tgz",
    "@machinat/jest-snapshot-serializer": "file:machinat-jest-snapshot-serializer-0.3.0-beta.2.tgz",
    "@machinat/line": "file:machinat-line-0.3.0-beta.3.tgz",
    "@machinat/messenger": "file:machinat-messenger-0.3.0-beta.3.tgz",
    "@machinat/next": "file:machinat-next-0.3.0-beta.3.tgz",
    "@machinat/redis-state": "file:machinat-redis-state-0.3.0-beta.2.tgz",
    "@machinat/script": "file:machinat-script-0.3.0-beta.3.tgz",
    "@machinat/simple-state": "file:machinat-simple-state-0.3.0-beta.3.tgz",
    "@machinat/telegram": "file:machinat-telegram-0.3.0-beta.3.tgz",
    "@machinat/websocket": "file:machinat-websocket-0.3.0-beta.3.tgz",
    "@machinat/x-machinat": "file:machinat-x-machinat-0.3.0-beta.0.tgz",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "axios": "^0.19.2",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0",
    "draft-js": "^0.11.7",
    "next": "^9.5.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "symbol-observable": "^1.2.0",
    "umzug": "^3.0.0-beta.5",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "nodemon": ">=1.19.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
