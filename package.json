{
  "name": "note-example",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./lib ./dist && rm tsconfig.tsbuildinfo",
    "migrate": "per-env",
    "migrate:development": "ts-node ./src/cli/migrate.ts",
    "migrate:production": "node ./lib/cli/migrate.js",
    "build": "npm run build:source && npm run build:webview && npm run build:next",
    "build:source": "tsc",
    "build:webview": "mkdir -p ./lib/webview && cp ./src/webview/next.config.js ./lib/webview/",
    "build:next": "next build ./src/webview",
    "start": "per-env",
    "start:development": "ts-node ./src/index.ts",
    "start:production": "node ./lib/index.js",
    "dev": "ts-node ./src/cli/dev.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@machinat/core": "^0.4.1-alpha.40",
    "@machinat/dialogflow": "^0.4.1-alpha.34",
    "@machinat/http": "^0.4.1-alpha.34",
    "@machinat/line": "^0.4.1-alpha.38",
    "@machinat/local-state": "^0.4.1-alpha.34",
    "@machinat/messenger": "^0.4.1-alpha.38",
    "@machinat/redis-state": "^0.4.1-alpha.34",
    "@machinat/script": "^0.4.1-alpha.38",
    "@machinat/stream": "^0.4.1-alpha.39",
    "@machinat/telegram": "^0.4.1-alpha.38",
    "@machinat/webview": "^0.4.1-alpha.38",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.21.1",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0",
    "draft-js": "^0.11.7",
    "next": "^10.1.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "styled-components": "^4.3.2",
    "umzug": "^3.0.0-beta.15"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.2",
    "localtunnel": "^2.0.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
